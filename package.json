{
  "name": "@tinyhttp/cookie-parser",
  "version": "2.0.5",
  "type": "module",
  "description": "Cookie parsing middleware for Node.js",
  "homepage": "https://github.com/tinyhttp/cookie-parser#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/tinyhttp/cookie-parser.git"
  },
  "engines": {
    "node": ">=12.4 || 14.x || >=16"
  },
  "types": "./dist/index.d.ts",
  "exports": "./dist/index.js",
  "keywords": [
    "tinyhttp",
    "node.js",
    "web framework",
    "web",
    "backend",
    "cookie",
    "cookie-parser"
  ],
  "author": "v1rtl",
  "license": "MIT",
  "dependencies": {
    "@tinyhttp/cookie": "^2.0.3",
    "@tinyhttp/cookie-signature": "^2.0.3"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rollup -c",
    "test": "tsm node_modules/uvu/bin.js tests",
    "test:coverage": "c8 --include=src pnpm test",
    "test:report": "c8 report --reporter=text-lcov > coverage.lcov",
    "lint": "eslint . --ext=ts",
    "format": "prettier --check \"./**/*.{ts,md}\"",
    "format:fix": "prettier --write \"./**/*.{ts,md}\"",
    "prepare": "pnpm build && pnpm lint && pnpm test"
  },
  "devDependencies": {
    "@rollup/plugin-typescript": "^8.3.0",
    "@tinyhttp/app": "2.0.16",
    "@types/node": "^17.0.18",
    "@typescript-eslint/eslint-plugin": "^5.12.0",
    "@typescript-eslint/parser": "^5.12.0",
    "c8": "^7.11.0",
    "eslint": "^8.9.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "prettier": "^2.5.1",
    "rollup": "^2.67.2",
    "supertest-fetch": "^1.5.0",
    "tsm": "^2.2.1",
    "typescript": "^4.5.5",
    "uvu": "^0.5.3"
  }
}
